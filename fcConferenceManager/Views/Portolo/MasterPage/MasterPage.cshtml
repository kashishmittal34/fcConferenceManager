                                                <col style="width:35px">
                                                <col style="width:50px">
                                                <col>
                                                <col style="width:35px">
                                            </colgroup>
                                            <thead>
                                                <tr style="background-color:deepskyblue;color:white">
                                                    <th style="text-align:center;">
                                                        <input type="checkbox" id="selecctall" />
                                                    </th>
                                                    <th style="text-align: center;">
                                                        @Html.DisplayNameFor(m => Model.LookUps.FirstOrDefault().Id)
                                                    </th>
                                                    <th style="text-align: center;">
                                                        @Html.DisplayNameFor(m => Model.LookUps.FirstOrDefault().Name)
                                                    </th>
                                                    <th style="text-align: center;">Edit</th>
                                                </tr>
                                            </thead>
                                            <tbody style="display:none;"><tr><td colspan="4"></td></tr></tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="rgDataDiv" style="overflow-y:scroll;width:100%;height:270px;">

                                    <table style="width:100%;table-layout:fixed;overflow:hidden;empty-cells:show;">
                                        <colgroup>
                                            <col style="width:35px">
                                            <col style="width:50px">
                                            <col>
                                            <col style="width:35px">
                                        </colgroup>
                                        <thead style="display:none;">
                                            <tr>
                                                <th scope="col"></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Model.LookUps.Count() == 0)
                                            {

                                                <tr>

                                                    <td colspan="4">
                                                        NO RECORD !!!
                                                    </td>
                                                </tr>
                                            }
                                            else
                                            {

                                                foreach (var item in Model.LookUps)
                                                {

                                                    <tr style="height:25px">
                                                        <td>
                                                            <input class="checkbox" type="checkbox" name="check" value="@item.Id" style="margin:0 auto" onchange="showDeleteButton()" />
                                                        </td>
                                                        <td style="text-align: center;">
                                                            @Html.DisplayFor(modelItem => item.Id)
                                                        </td>
                                                        <td style="text-align: center;">
                                                            @Html.DisplayFor(modelItem => item.Name)
                                                        </td>
                                                        <td style="text-align: center;">
                                                            <a onclick="editNameLookUp(this)" data-Id="@item.Id" data-Name="@item.Name" style="border:hidden; background:white"><img src="~/Images/gridgray.png" width="22" /></a>
                                                        </td>
                                                    </tr>
                                                }
                                            }

                                        </tbody>

                                    </table>
                                </div>

                            </div>

                        </td>
                    </tr>
                </table>


