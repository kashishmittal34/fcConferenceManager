    function validateInputs(e) {

			 
    var Salutation1 = $.trim($('#Salutation1').val());
    var firstName = $.trim($('#firstNameid').val());
    var LastName = $.trim($('#LastName').val());
    var signinaccountid = $.trim($('#signinaccountid').val());
    var MainEmail = $.trim($('#MainEmail').val());
    var MainEmailType = $.trim($('#MainEmailType').val());
    var ConfirmPassword = $.trim($('#ConfirmPassword').val());
    var Password = $.trim($('#Password').val());
    var Address1 = $.trim($('#Address1').val());
    var city = $.trim($('#city').val());
    var state = $.trim($('#state').val());
    var zipcode = $.trim($('#zipcode').val());
    var jobtitle = $.trim($('#jobtitle').val());
    var Organization = $.trim($('#Organization').val());
    var phone1 = $.trim($('#phone1').val());

    if (Salutation1 == "") {
        $("#Salutation1").focus();
        alert("Please Select Salutation");
        return false;
    }
    if (firstName == "") {
        $("#firstNameid").focus();
        alert("Please Fill First Name");
        return false;
    }
    else if (LastName == "") {
        $("#LastName").focus();
        alert("Please Fill Last Name");
        return false;
    }
    else if (signinaccountid == "") {
        $("#signinaccountid").focus();
        alert("Please Fill Sign in Account ID");
        return false;
    }
    else if (MainEmail == "") {
        $("#MainEmail").focus();
        alert("Please Fill Main Email");
        return false;
    }
    else if (MainEmailType == "") {
        $("#MainEmailType").focus();
        alert("Please Fill MainEmailType");
        return false;
    }

    else if (Password == "" && "@string.IsNullOrEmpty(Model.ID)" != "False")
    {
        $("#Password").focus();
        alert("Please Fill Password");
        return false;
    }
    else if (ConfirmPassword == "" && "@string.IsNullOrEmpty(Model.ID)" != "False")
    {
        $("#ConfirmPassword").focus();
        alert("Please Fill Confirm Password");
        return false;
    }
    else if (Password != ConfirmPassword)
    {
        $("#Password").focus();
        $("#ConfirmPassword").focus();
        alert("Password and ConfirmPassword value not match.");
        return false;
    }

    else if (phone1 == "") {
        $("#phone1").focus();
        alert("Please Fill Phone 1");
        return false;
    }
    else if (Address1 == "") {
        $("#Address1").focus();
        alert("Please Fill Address1");
        return false;
    }
    else if (city == "") {
        $("#city").focus();
        alert("Please Fill City");
        return false;
    }
    else if (state == "") {
        $("#state").focus();
        alert("Please Select State");
        return false;
    }
    else if (zipcode == "") {
        $("#zipcode").focus();
        alert("Please Fill ZIP code");
        return false;
    }
    else if (jobtitle == "") {
        $("#jobtitle").focus();
        alert("Please Fill Job Title");
        return false;
    }

    else if (Organization == "") {
        $("#Organization").focus();
        alert("Please Fill Organization");
        return false;
    }
    else {
        return true
    }

}

